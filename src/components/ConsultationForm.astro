					submitButton.disabled = false;
					loadingText.classList.add("hidden");
					return;
				}

				const formData = new FormData(form);
				const formDataObject = Object.fromEntries(formData.entries());
				const lead = mapFormDataToLead(formDataObject);

				console.log("Form submitted with the following data:");
				console.log(JSON.stringify(lead));

				loadingText.classList.add("hidden");
				stepForm.classList.add("hidden");
				stepThanks.classList.remove("hidden");

				fetch("https://api.rodzinabezdlugow.pl/contact/leads", {
					method: "POST",
					headers: { "Content-Type": "application/json" },
					body: JSON.stringify(lead),
				})
					.then((res) => {
						if (!res.ok) throw new Error(`Status ${res.status}`);

						setTimeout(() => {
							window.open("/thank-you", "_self");
						}, 2000);

						form.reset();
					})
					.catch((err) => {
						console.error("Lead submission failed:", err);
					})
					.finally(() => {
						submitButton.disabled = false;
						loadingText.classList.add("hidden");
					});
			});
		}

		function mapFormDataToLead(formData) {
			const urlParams = new URLSearchParams(window.location.search);
			const props = [
				{
					name: "form_version",
					value: "rbd_com_consultations",
					type: "string",
				},
			];

			["utm_source", "utm_medium", "utm_campaign"].forEach((k) => {
				const v = urlParams.get(k);
				if (v) props.push({ name: k, value: v, type: "string" });
			});

			const tags = formData["debt-amount"] ? [formData["debt-amount"]] : [];

			return {
				id: (Date.now() / 1000) | 0,
				name: formData.name,
				email: formData.email,
				phone: formData.phone,
				createdOn: (Date.now() / 1000) | 0,
				properties: props,
				tags,
			};
		}
	});
</script>
